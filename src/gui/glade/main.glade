    <property name="border_width">5</property>
    <property name="icon">../../images/favicon.png</property>
    <property name="type_hint">normal</property>
    <property name="has_separator">False</property>
    <property name="program_name">Resistencia en C&#xE1;diz 1812</property>
    <property name="copyright" translatable="yes">Copyright &#xA9; 2009 Pablo Recio Quijano</property>
    <property name="comments" translatable="yes">Sistema Experto para ense&#xF1;ar Sistemas Expertos</property>
